#include <stdio.h>

int main() {
    int num1 = 2, num2 = 6, sum;

    // Calculate the sum
    sum = num1 + num2;

    // Display the result
    printf("The sum of %d and %d is: %d\n", num1, num2, sum);

    return 0;
}



   // second code

#include <stdio.h>

int main() {
    int num1, num2, sum;

    // Ask the user to input two numbers
    printf("Enter the first number: ");
    scanf("%d", &num1);
    printf("Enter the second number: ");
    scanf("%d", &num2);

    // Calculate the sum of the numbers
    sum = num1 + num2;

    // Display the result
    printf("The sum of %d and %d is: %d\n", num1, num2, sum);

    return 0;
}

